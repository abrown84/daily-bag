{
  "name": "daily-bag",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:pwa": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest --config jest.config.mjs --passWithNoTests",
    "test:watch": "jest --config jest.config.mjs --watch",
    "test:coverage": "jest --config jest.config.mjs --coverage",
    "test:ci": "jest --config jest.config.mjs --ci --coverage --watchAll=false",
    "video:studio": "remotion studio video/src/index.ts --public-dir=public",
    "video:render": "remotion render video/src/index.ts DailyBagIntro video/output/intro.mp4 --public-dir=public",
    "video:render-16x9": "remotion render video/src/index.ts DailyBagIntro-16x9 video/output/intro-16x9.mp4 --public-dir=public",
